---
const links = [
  { href: "/", label: "Home" },
  { href: "/projects", label: "Projects" },
  { href: "/experiences", label: "Experiences" },
  { href: "/resume", label: "Resume" },
  { href: "/notes", label: "Notes" },
  { href: "/about", label: "About" },
];
---
<nav class="flex items-center justify-between py-6">
  <a class="font-semibold text-lg" href="/">Alex Doyoon Kim</a>
  <ul class="flex flex-wrap gap-4 text-sm text-zinc-700">
    {links.map((l) => (
      <li><a class="hover:text-zinc-900 hover:underline" href={l.href}>{l.label}</a></li>
    ))}
  </ul>
</nav>
